{
  "name": "@subnoto/n8n-nodes-subnoto",
  "version": "0.1.0",
  "description": "n8n node to integrate with Subnoto for document signing",
  "license": "MIT",
  "homepage": "https://subnoto.com/documentation/developers/sdks/typescript",
  "keywords": [
    "n8n-community-node-package",
    "subnoto",
    "signature",
    "document"
  ],
  "author": {
    "name": "Subnoto",
    "email": "support@subnoto.com"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "index.js",
  "engines": {
    "node": ">=20.19 <=24.x"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "format": "prettier --write \"**/*.{ts,js,mjs,json}\"",
    "format:check": "prettier --check \"**/*.{ts,js,mjs,json}\"",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": false,
    "credentials": [
      "dist/credentials/SubnotoApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Subnoto/Subnoto.node.js"
    ]
  },
  "dependencies": {
    "@subnoto/api-client": "2.3.10"
  },
  "devDependencies": {
    "@n8n/node-cli": "0.19.0",
    "eslint": "9.39.2",
    "prettier": "3.8.1",
    "release-it": "19.2.4",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "overrides": {
    "change-case": "4.1.2"
  }
}